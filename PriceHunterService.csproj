    </Reference>
    <Reference Include="MongoDB.Libmongocrypt">
      <HintPath>MongoDB.Libmongocrypt.dll</HintPath>
    </Reference>
    <Reference Include="PriceHunterFilter">
      <HintPath>PriceHunterFilter.dll</HintPath>
    </Reference>
    <Reference Include="SharpCompress">
      <HintPath>SharpCompress.dll</HintPath>
    </Reference>
    <Reference Include="Snappier">
      <HintPath>Snappier.dll</HintPath>
    </Reference>
    <Reference Include="ZstdSharp">
      <HintPath>ZstdSharp.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
